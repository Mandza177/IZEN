  <div class="navbar navbar-expand-sm navbar-light ">
    <div class="container-fluid">
    <%= link_to "/" do %>
      <i class="fa-solid fa-house btn-home"></i>
    <% end %>
      <div class="log-area">
        <% if user_signed_in? %>
          <%= link_to "Log out", destroy_user_session_path, data: {turbo_method: :delete}, class: "log-out" %>
          <%= link_to profile_path(current_user) do %>
            <%= image_tag "svg/profile.svg" %>
          <% end %>
          <% else %>
            <%= link_to new_user_session_path do %>
            <%= image_tag "svg/profile.svg" %>
            <% end %>
          <% end %>
      </div>
    </div>
  </div>
</div>
